{
  "name": "nativescript-getters-root",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint --ext=.ts src/",
    "minify": "terser src/dist/index.js --output src/dist/index.min.js --comments all --compress",
    "dev": "tsc --watch --project src/",
    "build": "tsc --project src/",
    "clean": "ns clean",
    "debug:ios": "ns debug ios",
    "debug:android": "ns debug android",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@nativescript/eslint-plugin": "0.0.4",
    "@typescript-eslint/parser": "4.22.0",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.25.0",
    "nativescript": "8.0.1",
    "standard-version": "9.2.0",
    "terser": "5.6.1",
    "typescript": "4.2.4"
  }
}
