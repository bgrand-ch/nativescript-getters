{
  "private": true,
  "name": "nativescript-getters-monorepo",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "postprepare": "npm rebuild",
    "commit": "git-cz",
    "release": "standard-version",
    "clean": "ultra -r npx ns clean",
    "demo:debug": "ns debug --path ./packages/demo --",
    "demo-ng:debug": "ns debug --path ./packages/demo-ng --",
    "demo-svelte:debug": "ns debug --path ./packages/demo-svelte --",
    "demo-ts:debug": "ns debug --path ./packages/demo-ts --",
    "demo-vue:debug": "ns debug --path ./packages/demo-vue --"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@nativescript/eslint-plugin": "0.0.4",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.24.0",
    "husky": "4.3.8",
    "nativescript": "8.0.1",
    "standard-version": "9.2.0",
    "ultra-runner": "3.10.5"
  }
}
